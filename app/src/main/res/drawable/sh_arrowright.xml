<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:width="100dp" android:height="75dp"
        android:left="38dp">
        <shape>
            <solid android:color="#FFFFFF"/>
            <corners
                android:bottomLeftRadius="6dp"
                android:topLeftRadius="6dp"/>
        </shape>
    </item>

    <item android:width="53dp" android:height="53dp"
        android:left="138dp">
        <rotate android:fromDegrees="45"
            android:pivotX="0"
            android:pivotY="0">
            <shape>
                <solid android:color="#FFFFFF"/>
            </shape>
        </rotate>
    </item>

</layer-list>